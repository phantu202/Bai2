package lesson_3;

public class Test {

	public static void main(String[] args) {
		int a = 0;
		int [] arr = {1,2,3,4,5};
		int x = arr[2];			//Lấy giá trị mảng ra
		System.out.println(x);
		arr[2] = 10;
		System.out.println(arr[2]);
		int [] arr1 = new int[10];
		do {
			arr1[a] = a;
			a++;
		} while (a < 10);
		for (int i = 0; i < 10; i++) {
			System.out.println(arr1[i]);
		}
		
		
		
		
		
		
	}

}
