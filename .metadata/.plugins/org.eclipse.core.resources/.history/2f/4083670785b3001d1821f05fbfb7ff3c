package lesson_3;

import java.security.PublicKey;

public class BaiTap1 {
	//Tạo 1 mảng số nguyên, tìm số nhỏ nhất và lớn nhất trong mảng đó
	public static void main(String[] args) {
		int [] arr = {23,54,2,104,5,54,89};
	
		System.out.println(myFuntion(4, 5));
		System.out.println(testString(4, "Get out "));
		
	}

	public static int myFuntion(int a, int b) {
		int sum = a + b;
		return sum;
	}
	
	public static String testString (int i , String a) {
		return a + i;
	}
	

}
