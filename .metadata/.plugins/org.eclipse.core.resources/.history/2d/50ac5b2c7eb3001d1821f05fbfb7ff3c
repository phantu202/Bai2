package lesson_3;

public class BaiTap1 {
	//Tạo 1 mảng số nguyên, tìm số nhỏ nhất và lớn nhất trong mảng đó
	public static void main(String[] args) {
		int [] arr = {23,54,2,104,5,54,89};
		int min = arr[0];
		int max = arr[0];
		for (int i = 1; i < arr.length; i++) {
			if (min > arr[i]) {
				min = arr[i];
			}
			if (max < arr[i]) {
				max = arr[i];
			}
		}
		System.out.println("Min = " + min + " Max = " + max);
		

	}

}
